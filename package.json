{
  "name": "snowforce",
  "private": true,
  "version": "1.0.0",
  "description": "Snowforce Community Website",
  "scripts": {
    "start-dev-server": "stencil build --dev --watch --serve",
    "production-build": "stencil build",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "build-docs": "stencil build --docs",
    "compile-theme-scss": "node-sass theme-scss/main.scss src/global/app.css",
