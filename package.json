    "prefirebase-deploy": "stencil build",
    "firebase-deploy": "firebase deploy"
  },
  "dependencies": {
    "@stencil/core": "0.18.0",
    "@stencil/router": "0.3.3",
    "firebase": "^5.9.3"
  },
  "license": "MIT",
  "devDependencies": {
    "@stencil/sass": "^0.1.1",
    "@types/jest": "23.3.14",
    "@types/puppeteer": "1.12.3",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "puppeteer": "1.11.0",
    "workbox-build": "3.4.1"
  }
}
